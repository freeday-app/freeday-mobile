{
	"name": "freeday-mobile",
	"version": "1.0.0",
	"author": "Coddity",
	"description": "Freeday mobile app",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prepare": "husky install",
    "lint-js": "eslint . --max-warnings=0",
    "lint-js-fix": "eslint . --fix --max-warnings=0",
    "lint-md": "markdownlint README.md",
    "lint-md-fix": "markdownlint README.md --fix",
    "lint": "npm run lint-js && npm run lint-md",
    "lint-fix": "npm run lint-js-fix && npm run lint-md-fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/material-bottom-tabs": "^6.2.2",
    "@react-navigation/native": "^6.0.11",
    "dayjs": "^1.11.3",
    "expo": "^45.0.0",
    "expo-font": "~10.1.0",
    "expo-secure-store": "~11.2.0",
    "intl": "^1.2.5",
    "prop-types": "^15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-emoji": "^1.8.0",
    "react-native-paper": "^4.12.3",
    "react-native-paper-dates": "^0.8.7",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.1",
    "markdownlint": "^0.26.0",
    "markdownlint-cli": "^0.32.0"
  }
