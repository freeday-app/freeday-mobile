{
    "plugins": [
        "stylelint-order"
	],
    "extends": [
        "stylelint-config-standard"
    ],
    "rules": {
        "indentation": 4,
		"order/properties-alphabetical-order": true
	},
    "ignoreFiles": [
        ".expo/**/*",
        ".expo-shared/**/*",
        "node_modules/**/*",
        "**/*.js"
    ]
}
